# Globale DB (fallback pentru postgres_server)
SUITE_DB_POSTGRES_USER=suite_admin
SUITE_DB_POSTGRES_PASS=ReplaceMeWithStrongPostgresPassword

# SuperTokens API key (folosit atât de serviciul root, cât și de cp/identity)
CP_IDT_AUTH_SUPERTOKENS_API_KEY=ReplaceMeWithSuperTokensAPIKey

# Neo4j (vettify)
VETFY_DB_NEO4J_USER=neo4j
VETFY_DB_NEO4J_PASS=ReplaceMeWithNeo4jPassword
