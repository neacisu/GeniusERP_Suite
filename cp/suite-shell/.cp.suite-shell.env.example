# -- CONFIGURARE APLICAȚIE --
# Portul intern al BFF/Orchestrator MicroFrontend
CP_SHELL_APP_PORT=6100
CP_SHELL_APP_METRICS_PORT=6101
CP_SHELL_APP_NODE_ENV=production

# -- MICRO-FRONTEND --
CP_SHELL_MF_REGISTRY_URL=http://suite-shell:6100/mf-registry
CP_SHELL_MF_RUNTIME_MODE=production

# -- BAZA DE DATE --
CP_SHELL_DB_POSTGRES_URL=postgresql://shell_user:ReplaceMe@postgres_server:5432/suite_shell_db

# -- AUTENTIFICARE --
# Conexiune la serviciul Identity
CP_SHELL_AUTH_IDENTITY_URL=http://identity:6250
CP_SHELL_AUTH_JWT_SECRET=ReplaceMeWithStrongJWTSecretKey

# -- URL-URI SERVICII INTERNE --
CP_SHELL_SVC_ADMIN_URL=http://suite-admin:6150
CP_SHELL_SVC_LICENSING_URL=http://licensing:6300
CP_SHELL_SVC_ANALYTICS_URL=http://analytics-hub:6350

# -- OBSERVABILITATE --
CP_SHELL_OBS_OTEL_ENDPOINT=${SUITE_OBS_OTEL_COLLECTOR_GRPC_URL}

